<Project>
  <PropertyGroup>
    <NoWarn>$(NoWarn);S107;S2436</NoWarn>
  </PropertyGroup>
  <PropertyGroup Condition="'$(GeneratePackageOnBuild)' == 'true'">
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>
  <ItemGroup>
    <Using Include="System.ComponentModel" />
  </ItemGroup>
</Project>